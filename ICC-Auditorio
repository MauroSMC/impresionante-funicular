# impresionante-funicular
Trabajo ICC (auditorio)
def factorial(n):
    prod = 1
    for i in range(1, n+1):
        prod = prod * i

    return prod
def combinatoria(n, k):
    if n >=k:
        comb = factorial(n)/(factorial(k)*factorial(n-k))
        comb = int(comb)
        return comb
    else:
        return 0

print(combinatoria(8,3))
print(combinatoria(10,2))
print(combinatoria(90,100))



# Para saber si un n√∫mero es primo o no
n = int(input())
def EsPrimo(n):
    for c in range(2, n):
        if n % c ==0:
            return False
    return True
if EsPrimo(n):
    print("SI ES PRIMO")
else:
    print("NO ES PRIMO")
